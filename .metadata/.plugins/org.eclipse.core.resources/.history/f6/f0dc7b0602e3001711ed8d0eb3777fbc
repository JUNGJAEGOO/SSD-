package back1024;

import java.math.BigDecimal;
import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int L = in.nextInt();
	
		int n = 2;
		while( n < 101) {
			
			double a = (N - (n*(n-1))/2)/n);
			System.out.println(a);
			
			n++;
		}
	}
}
