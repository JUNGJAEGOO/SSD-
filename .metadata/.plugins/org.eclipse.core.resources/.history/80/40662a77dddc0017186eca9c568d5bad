package back2342;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int arr[] = new int[100001];
		int dp[][] = new int[2][100001];
		
		int count=1;
		while( true) {
			 int x= in.nextInt();
			 if ( x == 0) {
				 break;
			 }
			 arr[count++] = x;
			
		}
		
		int Lbeforestate = 0;
		int Rbeforestate = 0;
		for( int i = 1 ; i < count ; i++) {
			
			
			dp[0][i] = Math.min(dp[1][i-1], dp[0][i-1]);
					
			
			
		}
		
		
		
	}
}
