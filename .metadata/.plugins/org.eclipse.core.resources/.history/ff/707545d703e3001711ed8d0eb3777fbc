package back1024;

import java.math.BigDecimal;
import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		double N = in.nextInt();
		double L = in.nextInt();
	
		double n = 1;
		while( n < 101) {
			n++;
			double a = ((N - (n*(n-1))/2)/n);
			System.out.println(a);
			String tmp = String.valueOf(a);
			boolean pass = isInteger(tmp);
			if ( pass == false ) { break; }
			
		}
		
		System.out.println(n);
	}
	
	public static boolean isInteger(String a) {
		System.out.println(a);
		String tmptmp[] = a.split(".");
		System.out.println(a.split(".")[0]);
		try {
			String res[] = a.split(".");
			System.out.println(res[1]);
			if ( Integer.parseInt(res[1]) == 0) {
				return false;
			}
			return true;
		}catch (Exception e) {
			return false;
		}
		
	}
}
