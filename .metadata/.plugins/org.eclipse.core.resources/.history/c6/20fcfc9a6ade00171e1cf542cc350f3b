package back14501;

import java.util.Scanner;

public class Main {
	static int t[];
	static int p[];
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		t = new int[N+1];
		p = new int[N+1];

		
		for (int i = 1 ; i <= N ; i++) {
			t[i] = in.nextInt();
			p[i] = in.nextInt();
		}
		

	}
	
	public static void back(int start,int len) {
		
	}
}
