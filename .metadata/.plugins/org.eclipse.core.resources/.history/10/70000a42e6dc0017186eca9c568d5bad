package back2352;

import java.util.*;

public class Main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		
		ArrayList<Integer> list = new ArrayList<>();
		int arr[] = new int[N];
		for ( int i = 0 ; i < N ; i++) {
			arr[i] = in.nextInt();
		}
		
		list.add(arr[0]);
		int size = 1;
		for ( int i = 1 ; i < N ; i++) {
			if ( list.get(size-1) < arr[i]){
				list.add(arr[i]);
				size++;
			}else {
				list.remove(size-1);
				
				list.add(arr[i]);
			}
		}
		
		for ( int i = 0 ; i < list.size() ; i++) {
			System.out.print(list.get(i)+" ");
		}
		System.out.println(list.size());
	}
}
