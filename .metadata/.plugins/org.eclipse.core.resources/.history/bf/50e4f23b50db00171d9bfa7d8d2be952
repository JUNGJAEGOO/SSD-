package bak14568;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int ans = 0;
		if ( N <= 5 ) {
			System.out.println(0);
			return;
		}else {
			
				
				for ( int i = 2 ; i<100 ; i=i*2) {
					int tmp = N;
					tmp -= i;
					if ( tmp%2 == 0 && tmp>=4) {
						ans++;
					}
				}
				
				
			System.out.pritnln(ans);
			
			
		}
	}
}
