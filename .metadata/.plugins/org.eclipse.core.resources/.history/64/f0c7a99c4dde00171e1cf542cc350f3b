package back14723;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		Scanner in  =new Scanner(System.in);
		int N = in.nextInt();
		int start = 1;
		int d = 1;
		int st[] = new int[100];
		while ( true) {
			start = start+d;
			d++;
			if ( start > 1000) {
				break;
			}
			st[d-1] = start;
		}
		
	}
}
