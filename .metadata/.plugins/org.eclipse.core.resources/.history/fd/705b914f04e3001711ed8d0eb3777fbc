package back1024;

import java.math.BigDecimal;
import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		double N = in.nextInt();
		double L = in.nextInt();
	
		double n = L-1;
		double a;
		while( n < 101) {
			n++;
			double a = ((N - (n*(n-1))/2)/n);
			//System.out.println(a);
			String tmp = String.valueOf(a);
			boolean pass = isInteger(tmp);
			if ( pass == false ) { break; }
			
		}
		
		for ( int i = 0 ; i < n ; i++) {
			System.out.print(a+n);
		}
		
	}
	
	public static boolean isInteger(String a) {

		try {
			String res[] = a.split("\\.");
			//System.out.println(res[1]);
			if ( Integer.parseInt(res[1]) == 0) {
				return false;
			}
			return true;
		}catch (Exception e) {
			return false;
		}
		
	}
}
