package back1024;

import java.math.BigDecimal;
import java.util.Scanner;

public class main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		double N = in.nextInt();
		double L = in.nextInt();
	
		double n = L-1;
		double a = 0;
		while( n < 101) {
			n++;
			a = ((N - (n*(n-1))/2)/n);
			//System.out.println(a);
			String tmp = String.valueOf(a);
			boolean pass = isInteger(tmp);
			if ( pass == false ) { break; }
			
		}
		
		if (  n > 100) {
			System.out.println(-1);
			return;
		}
		
		for ( int i = 0 ; i < n ; i++) {
			System.out.print((int)(a+i)+" ");
		}
		
	}
	
	public static boolean isInteger(String a) {

		
			String res[] = a.split("\\.");
			//System.out.println(res[1]);
			if ( res[1].equals("0")) {
				
				return false;
			}
			return true;
		/*catch (Exception e) {
			System.out.print("hi");
			return false;
		}*/
		
	}
}
