package back2352;

import java.util.*;

public class Main {
	public static void mina(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		ArrayList<Integer> list = new ArrayList<>();
		int arr[] = new int[N];
		for ( int i = 0 ; i < N ; i++) {
			arr[i] = in.nextInt();
			System.out.println(arr[i]);
		}
		
		list.add(arr[0]);
		System.out.println(list.get(0));
		int size = 1;
		for ( int i = 1 ; i < N ; i++) {
			if ( list.get(size-1) < arr[i]){
				System.out.println(arr[i]);
				list.add(arr[i]);
				size++;
			}else {
				System.out.println("alt"+list.get(size-1)+"to"+arr[i]);
				list.remove(size-1);
				
				list.add(arr[i]);
			}
		}
		
		System.out.println(list.size());
	}
}
