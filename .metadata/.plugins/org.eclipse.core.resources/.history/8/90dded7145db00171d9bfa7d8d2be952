package back14927;

import java.util.Scanner;

public class Main {
	public static void main(String args[]) {
		Scanner in = new Scanner(System.in);
		int N = in.nextInt();
		int mat[][]= new int[N][N];
		
		for( int i = 0 ; i < N ; i++) {
			for ( int j = 0 ; j < N ;j++) {
				mat[i][j] = in.nextInt();
			}
		}
		
		for ( int i = 0 ; i < N ; i++) {
			int sum = 0;
			for ( int j = 0 ; j < N ;j++) {
				sum+= mat[i][j];
			}
			if ( sum != 0) {
				for ( int s = 0 ; s< N ;s++) {
					if ( mat[i][s] == 1) {
						mat[i+1][s] = 1 - mat[i+1][s] ;
					}
				}
			}
		}
		
	}
}
